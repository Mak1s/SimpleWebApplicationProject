<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetStudent</servlet-name>
        <servlet-class>servlets.GetStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emailServlet</servlet-name>
        <servlet-class>servlets.emailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usernameServlet</servlet-name>
        <servlet-class>servlets.usernameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pasoServlet</servlet-name>
        <servlet-class>servlets.pasoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetLibrarian</servlet-name>
        <servlet-class>servlets.GetLibrarian</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostLogin</servlet-name>
        <servlet-class>servlets.PostLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUser</servlet-name>
        <servlet-class>servlets.GetUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllUsers</servlet-name>
        <servlet-class>servlets.GetAllUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteUser</servlet-name>
        <servlet-class>servlets.deleteUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetStudent</servlet-name>
        <url-pattern>/GetStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emailServlet</servlet-name>
        <url-pattern>/emailServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usernameServlet</servlet-name>
        <url-pattern>/usernameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pasoServlet</servlet-name>
        <url-pattern>/pasoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetLibrarian</servlet-name>
        <url-pattern>/GetLibrarian</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostLogin</servlet-name>
        <url-pattern>/PostLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUser</servlet-name>
        <url-pattern>/GetUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllUsers</servlet-name>
        <url-pattern>/GetAllUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteUser</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
